{
    "name": "template-service",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "generate-grpc": "./node_modules/.bin/proto-loader-gen-types --longs=Number --enums=Number --oneofs --grpcLib=@grpc/grpc-js --outDir=src/proto/gen src/proto/**/*.proto",
        "clean": "rimraf ./src/proto/gen && rimraf .dist",
        "build": "npm run clean && npm run generate-grpc && npx tsc --build",
        "build-clean": "npm run clean && tsc --build --clean",
        "dev-initialize": "ts-node-dev src/main.ts --initialize",
        "dev": "ts-node-dev src/main.ts --start_http_server",
        "dev-grpc": "ts-node-dev src/main.ts --start_grpc_server",
        "start-initialize": "node dist/main.js --prod --initialize",
        "start": "node dist/main.js --prod --start_http_server",
        "start-grpc": "node dist/main.js --prod --start_grpc_server",
        "prettier-format": "prettier --write .",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@grpc/grpc-js": "^1.10.0",
        "@grpc/proto-loader": "^0.7.10",
        "@types/cookie-session": "^2.0.48",
        "@types/express": "^4.17.21",
        "bcrypt": "^5.1.1",
        "brandi": "^5.0.0",
        "cookie-session": "^2.1.0",
        "dotenv": "^16.4.4",
        "express": "^4.18.2",
        "express-async-errors": "^3.1.1",
        "jsonwebtoken": "^9.0.2",
        "kafkajs": "^2.2.4",
        "knex": "^3.1.0",
        "minimist": "^1.2.8",
        "ms": "^2.1.3",
        "mysql2": "^3.9.1",
        "node-cache": "^5.1.2",
        "nodejs-snowflake": "^2.0.1",
        "pg": "^8.11.3",
        "reflect-metadata": "^0.2.1",
        "rimraf": "^5.0.5",
        "typeorm": "^0.3.20",
        "winston": "^3.11.0",
        "winston-daily-rotate-file": "^5.0.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/minimist": "^1.2.5",
        "@types/node": "^20.11.17",
        "prettier": "^3.2.5",
        "ts-node-dev": "^2.0.0",
        "ts-protoc-gen": "^0.15.0",
        "typescript": "^5.3.3"
    }
}
